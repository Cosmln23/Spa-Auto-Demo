{
  "name": "spa-auto-demo",
  "private": true,
  "version": "1.0.0",
  "description": "Professional Spa Auto booking system with AI voice assistant",
  "author": "Cosmln23",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cosmln23/Spa-Auto-Demo.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "docs:server": "node Documentatie/docs/local-server.js",
    "docs:download": "powershell -ExecutionPolicy Bypass -File Documentatie/docs/docs-download.ps1",
    "prepare": "husky install || true",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "next": "^15.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@types/node": "24.3.1",
    "@types/react": "18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
